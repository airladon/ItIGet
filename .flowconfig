[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.md
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
module.name_mapper.extension='md' -> 'empty/object'

[ignore]
<PROJECT_ROOT>/src/content/boilerplate/.*
<PROJECT_ROOT>/env/.*
<PROJECT_ROOT>/migrations/.*
<PROJECT_ROOT>/containers/.*
<PROJECT_ROOT>/app/.*
